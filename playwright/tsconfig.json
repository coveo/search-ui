{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "allowImportingTsExtensions": true,
        "experimentalDecorators": true,
        "skipLibCheck": true,
        "noEmit": true,
        "checkJs": true,
        "allowJs": true,
        "baseUrl": ".",
        "target": "ESNext",
        "module": "commonjs",
        "moduleResolution": "Node",
        "sourceMap": true,
        "outDir": "./e2e",
        "lib": [
            "es2016",
            "dom",
            "ES2018.Intl",
            "dom.Iterable",
            "ES2021.String",
            "ES5"
            
        ]
    }
}